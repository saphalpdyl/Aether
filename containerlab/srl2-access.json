{
  "interface": [
    {
      "name": "ethernet-1/1",
      "admin-state": "enable",
      "subinterface": [
        {
          "index": 0,
          "admin-state": "enable",
          "type": "bridged"
        }
      ]
    },
    {
      "name": "ethernet-1/2",
      "admin-state": "enable",
      "subinterface": [
        {
          "index": 0,
          "admin-state": "enable",
          "type": "bridged"
        }
      ]
    },
    {
      "name": "ethernet-1/3",
      "admin-state": "enable",
      "subinterface": [
        {
          "index": 0,
          "admin-state": "enable",
          "ipv4": {
            "admin-state": "enable",
            "address": [
              {
                "ip-prefix": "10.0.0.3/24"
              }
            ]
          }
        }
      ]
    },
    {
      "name": "irb1",
      "subinterface": [
        {
          "index": 1,
          "admin-state": "enable",
          "ipv4": {
            "admin-state": "enable",
            "address": [
              {
                "ip-prefix": "10.0.1.65/26"
              }
            ],
            "dhcp-relay": {
              "admin-state": "enable",
              "gi-address": "10.0.1.65",
              "use-gi-addr-as-src-ip-addr": true,
              "relay-unicast-for-renew": true,
              "server": [
                "10.0.0.1"
              ],
              "option": [
                "circuit-id",
                "remote-id"
              ]
            }
          }
        }
      ]
    }
  ],
  "network-instance": [
    {
      "name": "access",
      "type": "mac-vrf",
      "admin-state": "enable",
      "interface": [
        { "name": "ethernet-1/1.0" },
        { "name": "ethernet-1/2.0" },
        { "name": "irb1.1" }
      ]
    },
    {
      "name": "default",
      "type": "default",
      "admin-state": "enable",
      "interface": [
        { "name": "ethernet-1/3.0" },
        { "name": "irb1.1" }
      ],
      "next-hop-groups": {
        "group": [
          {
            "name": "nhg-default",
            "admin-state": "enable",
            "nexthop": [
              {
                "index": 0,
                "ip-address": "10.0.0.1",
                "admin-state": "enable"
              }
            ]
          }
        ]
      },
      "static-routes": {
        "route": [
          {
            "prefix": "0.0.0.0/0",
            "admin-state": "enable",
            "next-hop-group": "nhg-default"
          }
        ]
      }
    }
  ],
  "system": {
    "name": {
      "host-name": "srl2-access"
    }
  }
}
